{
  "version": 2,
  "functions": {
    "api/vercel-server.js": {
      "maxDuration": 30,
      "includeFiles": "{*.html,*.css,*.js,*.json,js/**,css/**,images/**,manifest.json,sw.js}"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/vercel-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/vercel-server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  }
}