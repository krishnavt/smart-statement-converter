{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/vercel-server"
    },
    {
      "source": "/(.*)",
      "destination": "/api/vercel-server"
    }
  ],
  "functions": {
    "api/vercel-server.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}