{
  "version": 2,
  "functions": {
    "api/vercel-server.js": {
      "maxDuration": 30,
      "includeFiles": ["script.js", "manifest.json", "sw.js", "styles.css", "favicon.ico"]
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/vercel-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/vercel-server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  }
}